    <!-- 加载 页面 属性 LoadingPage-->
    <declare-styleable name="LoadingPage">
        <!-- Sting  提示内容-->
        <attr name="textWhenStaticLoading" format="string" />
        <attr name="textWhenError" format="string" />
        <attr name="textWhenNoNetwork" format="string" />
        <attr name="textWhenConnectTimeout" format="string" />
        <attr name="textWhenEmptyData" format="string" />

        <!--  提示内容,字体颜色-->
        <attr name="textColorWhenStaticLoading" format="reference" />
        <attr name="textColorWhenError" format="reference" />
        <attr name="textColorWhenNoNetwork" format="reference" />
        <attr name="textColorWhenConnectTimeout" format="reference" />
        <attr name="textColorWhenEmptyData" format="reference" />

        <!--  错误提示 图片-->
        <attr name="imageResIdWhenStaticLoading" format="reference" />
        <attr name="imageResIdWhenError" format="reference" />
        <attr name="imageResIdWhenNoNetwork" format="reference" />
        <attr name="imageResIdWhenConnectTimeout" format="reference" />
        <attr name="imageResIdWhenEmptyData" format="reference" />

    </declare-styleable>
